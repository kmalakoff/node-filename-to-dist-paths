{
  "name": "node-filename-to-dist-paths",
  "version": "0.1.4",
  "description": "Converts files from https://nodejs.org/dist/index.json into relative distribution paths for download from https://nodejs.org/dist/",
  "keywords": [
    "node",
    "version",
    "versions",
    "install",
    "dist",
    "release",
    "releases",
    "filename",
    "file",
    "files"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:kmalakoff/node-filename-to-dist-paths.git"
  },
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint .",
    "test": "mocha-compat test/spec/**/*.test.js --no-timeouts",
    "prepublishOnly": "dtd \"npm run lint\" \"depcheck --ignores='depcheck'\""
  },
  "dependencies": {
    "lodash.find": "^4.6.0",
    "match-semver": "^0.1.0",
    "object-assign": "^4.1.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "depcheck": "^0.9.2",
    "dis-dat": "^0.1.3",
    "end-of-stream": "^1.4.4",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "mocha-compat": "^3.5.5",
    "prettier": "^2.0.5",
    "queue-cb": "^1.1.4",
    "single-line-log": "^1.1.2"
  },
  "engines": {
    "node": ">=0.8"
  }
}
