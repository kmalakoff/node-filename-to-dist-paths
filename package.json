{
  "name": "node-filename-to-dist-paths",
  "version": "0.1.8",
  "description": "Converts files from https://nodejs.org/dist/index.json into relative distribution paths for download from https://nodejs.org/dist/",
  "keywords": [
    "node",
    "version",
    "versions",
    "install",
    "dist",
    "release",
    "releases",
    "filename",
    "file",
    "files"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:kmalakoff/node-filename-to-dist-paths.git"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint .",
    "prepublishOnly": "dtd \"npm run lint\" \"depcheck\"",
    "test": "mocha-compat test/spec/**/*.test.js --no-timeouts"
  },
  "dependencies": {
    "just-extend": "^6.0.1",
    "lodash.find": "^4.6.0",
    "match-semver": "^0.1.0"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^5.30.0",
    "depcheck": "^1.4.3",
    "dis-dat": "^0.1.7",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.3",
    "eslint-plugin-promise": "^6.0.0",
    "get-remote": "^0.6.3",
    "mocha-compat": "^3.5.5",
    "prettier": "^2.7.1",
    "queue-cb": "^1.1.6",
    "single-line-log2": "^1.1.3"
  },
  "engines": {
    "node": ">=0.8"
  }
}
