{
  "name": "node-filename-to-dist-paths",
  "version": "0.2.5",
  "description": "Converts files from https://nodejs.org/dist/index.json into relative distribution paths for download from https://nodejs.org/dist/",
  "keywords": [
    "node",
    "version",
    "versions",
    "install",
    "dist",
    "release",
    "releases",
    "filename",
    "file",
    "files"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:kmalakoff/node-filename-to-dist-paths.git"
  },
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsds build",
    "deploy": "tsds deploy",
    "format": "biome check --apply-unsafe src/ test/",
    "test": "tsds test:node --no-timeouts",
    "test:engines": "nvu engines npm test",
    "version": "tsds version"
  },
  "dependencies": {
    "lodash.find": "^4.6.0",
    "match-semver": "^0.1.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.5.3",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.19",
    "depcheck": "^1.4.7",
    "get-remote": "^0.8.4",
    "queue-cb": "^1.2.1",
    "single-line-log2": "^1.1.3",
    "ts-dev-stack": "^0.14.9"
  },
  "engines": {
    "node": ">=0.8"
  },
  "tsds": {
    "source": "src/index.mjs",
    "targets": [
      "cjs"
    ]
  }
}
